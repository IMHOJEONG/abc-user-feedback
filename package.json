{
  "name": "user-feedback",
  "version": "0.0.1",
  "description": "User voice feedback portal",
  "author": "high.alps@navercorp.com",
  "private": true,
  "license": "MIT",
  "scripts": {
    "pre-build": "rimraf dist .next",
    "start": "cross-env NODE_ENV=production node dist/server/main",
    "build": "yarn run pre-build && yarn run build:client && yarn run build:server",
    "build:client": "next build src/client",
    "build:server": "nest build -p tsconfig.server.json",
    "build:migration": "tsc src/server/database/migrations",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "dev": "nest start --watch -p tsconfig.server.json",
    "debug": "nest start --debug --watch",
    "lint": "eslint \"{server,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config src/server/test/jest-e2e.json"
  },
